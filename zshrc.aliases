alias mv="nocorrect mv"
alias cp="nocorrect cp"
alias racket="rlwrap racket"
alias rsync="rsync -P -avz"
alias sudo="nocorrect sudo"
alias time="/usr/bin/time -l"

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

alias b="bundle"                   # Bundler
alias e="$EDITOR"                  # Editor
alias l="ls -lh"                   # Pritty ls
alias ql="qlmanage -p 2>/dev/null" # Quick Look
alias tx="tmux attach || tenv"     # Attach or create tmux environment
alias v="$VISUAL"                  # Vim

alias webrick='ruby -run -e httpd . -p 3000'
